    |
    section.customers__main-box
        .customers__header
            .customers__header-title
                h2 All Customers
                |
                span Active Members
            .customers__header-search
                .icon
                    include /src/assets/img/search.svg
                input(type="search" placeholder="Search")

        .customers__table
            .customers__table-header
                    each name in tableTitle
                        span= name

            .customers__table-users
                each user in tableData
                    .customers__user-credentials
                        span= user.name
                        span= user.company
                        span= user.phone
                        span= user.email
                        span= user.country
                        span(class=(user.status === 'active') ? 'active' : 'inactive')= user.status

            .customers__table-footer
                span.data-info Showing data 1 to 8 of 256K entries
                |
                .customers__paginator
                    button
                        include /src/assets/img/chevron.svg
                    button.-active 1
